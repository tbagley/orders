
<!-- LANDMARK MAP AND LIST MODAL -->
{% if route == 'landmark/map' or route == 'landmark/list' %}

        <!-- Modal add landmark -->
        <div class="modal fade" id="modal-add-landmark" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        	<div class="modal-dialog">
        		<div class="modal-content" style="min-height:600px;">
                    <div class="modal-pronounce">
            			<div class="modal-header">
            				<button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
            				<h4 class="modal-title" id="modal-add-landmark-label">Add Landmark</h4>
                            {#}<span style="font-style:italic;text-grey-10">Enter an Address or Double Click on Map</span>{#}
            			</div>
            			<div class="modal-body">
                			<div class="col-md-6">
                                <div class="modal-map" id="addmap-div"></div>
                            </div>
        
                            <div class="col-md-6">

            					<div class="form-group row">
                                    <div class="col-md-2">
                                        <label class="control-label pull-right" for="landmark-add-name" style="margin-top:7px;">Name:</label>
                                    </div>
            						<div class="col-md-9">
            							<input type="text" id="landmark-add-name" name="landmark-add-name" class="form-control" placeholder="Name of Landmark">
            						</div>
            					</div>
            				
            					<div class="form-group row">
                                    <div class="col-md-2">
                                        <label class="control-label pull-right" for="landmark-add-group" style="margin-top:7px;">Group:</label>
                                    </div>
                                    <div class="col-md-9">
                                        <select class="form-control" id="landmark-add-group">
                                            {% for option in options_groups %}
                                                <option value="{{ option.v }}" data-value="{{ option.v }}">{{ option.k }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>
            					</div>

            					<div class="form-group row" style="display:none;">
                                    <div class="col-md-2">
                                        <label class="control-label pull-right" for="landmark-add-category" style="margin-top:2px;">Category:</label>
                                    </div>
                                    <div class="col-md-9 wizard-div" style="margin-top:1px;">
                                        <select class="form-control" id="landmark-add-category">
                                            {% for option in options_categories %}
                                                <option value="{{ option.v }}" data-value="{{ option.v }}">{{ option.k }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>
            					</div> 

                                <div class="form-group row">
                                    <div class="col-md-2">
                                        <label class="control-label pull-right" for="landmark-add-street_address" style="margin-top:7px;">Address:</label>
                                    </div>
                                    <div class="col-md-9">
                                        <input type="text" id="landmark-add-street_address" name="landmark-add-street_address" class="form-control" onKeyUp="Core.AddMap.Address(30);" placeholder="Street Address">
                                    </div>
                                </div> 
       
                                <div class="form-group row">
                                    <div class="col-md-2">
                                        <label class="control-label pull-right" for="landmark-add-city" style="margin-top:7px;">&nbsp;</label>
                                    </div>
                                    <div class="col-md-3">
                                        <input type="text" id="landmark-add-city" name="landmark-add-city" class="form-control" onKeyUp="Core.AddMap.Address(30);" placeholder="City">
                                    </div>
                                    <div class="col-md-2">
                                        <select class="form-control" id="landmark-add-state" onChange="Core.AddMap.Address(1);">
                                            {% for option in options_states %}
                                                <option value="{{ option.k }}" data-value="{{ option.k }}">{{ option.k }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>
                                    <div class="col-md-2">
                                        <input type="text" id="landmark-add-zip" name="landmark-add-zip" class="form-control" onKeyUp="Core.AddMap.Address(30);" placeholder="Zipcode">
                                    </div>
                                    <div class="col-md-2">
                                        <select class="form-control" id="landmark-add-country" onChange="Core.AddMap.Address(30);">
                                            {% for option in options_countries %}
                                                <option value="{{ option.k }}" data-value="{{ option.k }}">{{ option.k }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>
                                </div> 
          
                                <div class="form-group row">
                                    <div class="col-md-3">
                                        <label class="control-label pull-right text-grey" for="landmark-add-latlong" style="margin-top:7px;">or by Lat/Long</label>
                                    </div>
                                </div> 
          
                                <div class="form-group row">
                                    <div class="col-md-2">
                                        <label class="control-label pull-right" for="landmark-add-latlong" style="margin-top:7px;">&nbsp;</label>
                                    </div>
                                    <div class="col-md-5">
                                        <input type="text" id="landmark-add-latitude" name="landmark-add-latitude" class="form-control" onKeyUp="Core.AddMap.AddressReverse(30);" placeholder="Latitude">
                                    </div>
                                    <div class="col-md-4">
                                        <input type="text" id="landmark-add-longitude" name="landmark-add-longitude" class="form-control" onKeyUp="Core.AddMap.AddressReverse(30);" placeholder="Longitude">
                                    </div>
                                </div> 

                                <div class="form-group row">
                                    <div class="col-md-2">
                                        <label class="control-label pull-right" for="landmark-add-shape" style="margin-top:7px;">Shape:</label>
                                    </div>
                                    <div class="col-md-9">
                                        <select class="form-control" id="landmark-add-shape" onChange="Core.AddMap.Address(1);">
                                            {% for option in options_shapes %}
                                                <option value="{{ option.v }}" data-value="{{ option.v }}">{{ option.k }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>
                                </div>
                            
                                <div id="landmark-add-radius-row" class="form-group row">
                                    <div class="col-md-2">
                                        <label class="control-label pull-right" for="landmark-add-radius" style="margin-top:7px;">Radius:</label>
                                    </div>
                                    <div class="col-md-9">
                                        <select class="form-control" id="landmark-add-radius" onChange="Core.AddMap.Address(1);">
                                            {% for option in options_radius %}
                                                <option value="{{ option.v }}" data-value="{{ option.v }}">{{ option.k }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>
                                    <div class="col-md-1">
                                    </div>
{#}
                                    <label class="col-md-1 control-label" for="landmark-add-radius" style="margin-top:2px;">Lat/Lng:</label>
                                    <div class="col-md-5" style="margin-top:2px;">
                                        <span id="landmark-add-latitude" name="landmark-add-latitude">&nbsp;</span>
                                        &nbsp;/&nbsp;
                                        <span id="landmark-add-longitude" name="landmark-add-longitude">&nbsp;</span>
                                    </div>
{#}
                                </div>  
                            
                                <div id="landmark-add-radius-row" class="form-group row">
                                    <div class="col-md-2">
                                        <label class="control-label pull-right" for="landmark-add-radius" style="margin-top:7px;">Type:</label>
                                    </div>
                                    <div class="col-md-3">
                                        <select class="form-control" id="landmark-add-type">
                                            <option value="1">Impound Yard</option>
                                            <option value="0">Other</option>
                                        </select>
                                    </div>
                                    <div class="col-md-6" id="div-landmark-add-type-other" style="display:none;">
                                        <input class="form-control" id="landmark-add-type-other" placeholder="Type of Landmark...">
                                    </div>
                                    <div class="col-md-1">
                                    </div>
                                </div>  
                            
                                <div class="row">&nbsp;</div>
                                <div class="clearfix"> </div>

                                <div class="modal-footer row">
                                    <div class="col-md-1 pull-right">
                                        &nbsp;
                                    </div>
                                    <div class="col-md-1 pull-right">
                                        <button id="landmark-add" type="button" class="btn btn-primary create-update pull-right">Add Landmark</button>
                                    </div>
                                    <div class="col-md-1 pull-right">
                                        &nbsp;
                                    </div>
                                    <div class="col-md-1 pull-right">
                                        <button id="landmark-add-cancel" type="button" class="btn btn-default pull-right" data-dismiss="modal">Cancel</button>
                                    </div>
                                    <div class="col-md-4">
                                        <span id="landmark-error" class="center text-red">error msg</span>
                                    </div>
                                </div>

                			</div>
                        </div>
                    </div>
                </div>
        	</div>
        </div>
{% endif %}


