{% extends "layout/layout.html.twig" %}

{% block main_panel %}
    <!-- MAIN-PANEL BEGIN  -->

        <!-- FX Container -->
        <div id="fx-container" class="fx-opacity">
            <!-- Page content -->
            <!-- Block Tabs -->
            <div class="block block-tabs full">
                <!-- Block Tabs Title -->
                <div class="block-title">
                    {#% include 'partial/quickactions/vehicle-group-list.html.twig' %#}

                    <div class="block-options pull-right">
                        <a href="#modal-add-vehicle-group" data-toggle="modal" data-placement="bottom" onclick="Core.ClearForm('add-vehicle-group');" title="Add Vehicle Group" style="margin-right:15px;"><i class="gi gi-plus"></i> Vehicle Group </a>
                    </div>

                    <ul id="secondaryNav" class="nav nav-tabs">
                        {% for item in secondary_navigation %}
                            <li {% if route == item.route  %}class="active"{% endif %}><a href="/{{ item.route }}">{{ item.label }}</a></li>
                        {% endfor %}
                    </ul>

                </div>
                <!-- END Block Tabs Title -->

				<div class="table-options clearfix">
                    <div class="col-md-4 col-lg-4 col-sm-4 col-xs-6">  <h3 class="heading1">Vehicle Groups</h3></div> 
				</div>

            	<!-- END Table Options -->
                <div class="table-responsive">
                    <div class="dataTables_wrapper form-inline" role="grid">
                        <div id="vehicle-group-table" class="noborder report-master">
                            {% include('partial/report-table.html.twig') %}
{#}
                    <table id="example-datatable" class="table table-bordered table-hover">
                        <thead>
                            <tr>
                                <th style="font-size:14px;">Name</th>
                                <th style="font-size:14px;">Vehicle Count</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td style="width:50%;"><a href="#modal-edit-vehicle-group" data-toggle="modal">Default</a></td>
                                <td style="width:50%;">55</td>
                            </tr>
                        </tbody>
                    </table>
{#}
                        </div>
                    </div>
                </div>
                   
{#% include ('page/vehicle/detail.html.twig') %#}

            </div>
            <!-- END Block Tabs -->
            <!-- END Page Content -->
        </div>
        <!-- END FX Container -->

    <!-- MAIN-PANEL END  -->
{% endblock %}


{% block secondary_panel %}
    <!-- SECONDARY-PANEL BEGIN  -->
        {% include('partial/vehicle-sidebar.html.twig') %}
    <!-- SECONDARY-PANEL END  -->
{% endblock %}


{% block modals %}
    <!-- MODAL BEGIN -->
        {% include('partial/vehicle-modals.html.twig') %}
    <!-- MODAL END -->
{% endblock %}


{% block scripts_panel %}
    {#<script src="/assets_responsive/js/vehicle.js"></script>#}
    <script src="/assets/js/vehicle.js"></script>
{% endblock %}


