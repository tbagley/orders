                    {% set info_col       = 'col-info col-lg-4 col-md-4 col-sm-4 col-xs-4' %} {# outer/main columns #}
                    {% set label_col      = 'col-label col-lg-2 col-md-2 col-sm-2 col-xs-2' %} {# label columns #}
                    {% set control_col    = 'col-control col-lg-6 col-md-6 col-sm-6 col-xs-6' %} {# input/control columns #}
                    {% set control_offset = 'col-offset col-lg-4 col-md-4 col-sm-4 col-xs-4' %} {# allows spacing for x-editable button #}
                    {% set full_col       = 'col-lg-12 col-md-12 col-sm-12 col-xs-12' %}
                    {% set half_col       = 'col-lg-6 col-md-6 col-sm-6 col-xs-6' %}
                    {% set master_col     = 'col-lg-6 col-md-6 col-sm-6 col-xs-6' %}
                    {% set detail_col     = 'col-lg-6 col-md-6 col-sm-6 col-xs-6' %}


                    <div class="panel-body clearfix hook-editable-keys" data-user-type-pk="">

                        <div id="vehicle-group-user-detail" class="vehicle-group-user-detail">
            {# LABEL #}
                            <div class="row">
                                <div class="{{ full_col }}">
                                    <h5><span class="vehicle-group-user-title"></span>Assign User Access:</h5>
                                </div>
                                {#<div class="{{ half_col }}">&nbsp;</div>#}
                            </div>
            {# MASTER DETAIL HEADINGS #}
                            <div class="row">
                                <div class="{{ master_col }}">
                                    <div class="pull-left">
                                        <h5>
                                            <span class="master-detail-master-title">User Types</span>
                                        </h5>
                                    </div>
                                    <div class="pull-right">
                                        {#All&nbsp;#}<input id="vehicle-group-usertype-all" type="checkbox"/>
                                    </div>
                                </div>
                                <div class="{{ detail_col }}">
                                    <div class="pull-left">
                                        <h5>
                                            <span class="master-detail-detail-title">Users</span>
                                        </h5>
                                    </div>
                                    <div class="pull-right">
                                        {#All&nbsp;#}<input id="vehicle-group-user-all" type="checkbox"/>
                                    </div>
                                </div>
                            </div>
            {# MASTER DETAIL LIST #}
                            <div class="row">
                                <div class="{{ master_col }}">
                                    
                                    <ul id="vehicle-group-usertype-list" class="master-detail-list-master list-group">

                                    </ul>
                                    
                                </div>
                                <div class="{{ detail_col }}">
                                    <ul id="vehicle-group-user-list" class="master-detail-list-master list-group">

                                    </ul>
                                </div>
                            </div>
                        </div>

                    </div>

