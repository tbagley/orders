{% extends "layout/layout.html.twig" %}

{% block main_panel %}
    <!-- MAIN-PANEL BEGIN  -->

        <div id="fx-container" class="fx-opacity">
            <!-- Page content -->
            <!-- Block Tabs -->
            <div class="block block-tabs full">
                <!-- Block Tabs Title -->
                <div class="block-title">
                    <div id="quick-actions" class="block-options pull-right">
                        <a href="#modal-add-landmark-group" data-toggle="modal" data-placement="bottom" onclick="Core.ClearForm('landmarkgroup-add');" title="Add Landmark Group" style="margin-right:15px;"><span class="wtext"><i class="gi gi-plus"></i></span> LANDMARK GROUP</a>
                    </div>

                    <ul id="secondaryNav" class="nav nav-tabs">
                        {% for item in secondary_navigation %}
                            <li {% if route == item.route  %}class="active"{% endif %}><a href="/{{ item.route }}">{{ item.label }}</a></li>
                        {% endfor %}
                    </ul>
                </div>
                <!-- END Block Tabs Title -->
                <!-- END Block Tabs -->

                <div class="table-options clearfix">
                    <div class="col-md-4 col-lg-4 col-sm-4 col-xs-6">
                        <h3 class="heading1">Landmark Groups</h3>
                    </div> 
                    <div class="col-md-8 col-lg-8 col-sm-8 col-xs-6">
                        <div class="btn-group btn-group-sm pull-right">
                        </div>
                    </div>
                </div>

            	<!-- END Table Options -->
                <div class="table-responsive">
                    <div class="dataTables_wrapper form-inline" role="grid">
                        <div id="landmark-group-table" class="noborder report-master">
                            {% include('partial/report-table.html.twig') %}
{#}
                    <table id="landmark-group-example-datatable" class="table table-bordered table-hover">
                        <thead>
                            <tr>
                                <th style="font-size:14px;">Name</th>
                                <th style="font-size:14px;">Landmark Count</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td><a href="#modal-edit-landmark-group" data-toggle="modal">Default</a></td>
                                <td>20</td>

                            </tr>    
                        </tbody>
                    </table>
{#}
                        </div>
                    </div>
                </div>

            <!-- END FX Container -->

        </div>

    <!-- MAIN-PANEL END  -->
{% endblock %}


{% block secondary_panel %}
    <!-- SECONDARY-PANEL BEGIN  -->
        {% include('partial/landmark-sidebar.html.twig') %}
    <!-- SECONDARY-PANEL END  -->
{% endblock %}


{% block modals %}
    <!-- MODAL BEGIN -->
        {% include('partial/landmark-modals.html.twig') %}
    <!-- MODAL END -->
{% endblock %}


{% block scripts_panel %}
    <script src="/assets_responsive/js/landmark.js"></script>
{% endblock %}