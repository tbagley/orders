{% extends "layout/layout.html.twig" %}

{% block main_panel %}
    <!-- MAIN-PANEL BEGIN  -->
    
            <!-- FX Container -->
            <div id="fx-container" class="fx-opacity">
                <!-- Page content -->
                <!-- Block Tabs -->
                <div class="block block-tabs full">
                    <!-- Block Tabs Title -->
                    <div class="block-title">
                        <ul id="secondaryNav" class="nav nav-tabs">
                            {% for item in secondary_navigation %}
                                <li {% if route == item.route  %}class="active"{% endif %}><a href="/{{ item.route }}">{{ item.label }}</a></li>
                            {% endfor %}
                        </ul>
                    </div>
                    <!-- END Block Tabs Title -->

                    <!-- Tabs Content -->
                    <div class="tab-content">

                        <div class="table-options clearfix">
                            <div class="col-md-4 col-lg-4 col-sm-4 col-xs-6">
                                <h3 class="heading1">Report History</h3>
                            </div> 
                            <div class="col-md-8 col-lg-8 col-sm-8 col-xs-6">
                                <div class="btn-group btn-group-sm pull-right">
                                </div>
                            </div>
                        </div>

                        <div class="table-responsive">
                            <div class="dataTables_wrapper form-inline" role="grid">
                                <div id="report-history-table" class="noborder report-master">
                                    {% include('partial/report-table.html.twig') %}
        {#}
            						<table id="report-history-example-datatable" class="table table-bordered table-hover">
            							<thead>
            								<tr>
            									<th style="font-size:14px;">Report Type</th>
            									<th style="font-size:14px;">Report Name</th>
            									<th style="font-size:14px;">Scheduled/Manual </th>
