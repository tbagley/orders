            									<td>[9:59 pm 07/18/2014]</td>
            								   <td><a href="#"><span class="gi gi-new_window_alt"> </span></a></td>
            								</tr>    
            						  </tbody>
            						</table>
        {#}
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- END Tabs Content -->
                </div>
                <!-- END Block Tabs -->
                <!-- END Page Content -->
            </div>
            
    <!-- MAIN-PANEL END  -->
{% endblock %}


