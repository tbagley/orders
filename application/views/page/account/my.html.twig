{% extends "layout/layout.html.twig" %}

{% block main_panel %}
    <!-- MAIN-PANEL BEGIN  -->

    <div id="main-content">
        <div id="content-1" class="active panel-content modal-pronounce">
            <div class="secondary-bar clearfix">
                <div id="secondary-nav" class="pull-left">
                    <ul>
                        {% for item in secondary_navigation %}
                            <li><a class="{% if route == item.route  %} active{% endif %} transition" href="/{{ item.route }}">{{ item.label }}</a></li>
                        {% endfor %}
                    </ul>
                </div>
                {#{% include 'partial/quickactions/account-users.html.twig' %}#}
            </div>

            <div class="container">
                <div class="clearfix">
                    <div class="pull-left">
                        <h4><span class="icon account"></span>&nbsp;My Account</h4>
                    </div>
                </div>

            </div>
        </div>
    </div>
    <!-- MAIN-PANEL END  -->
{% endblock %}

